{
  "name": "black-screen",
  "version": "0.0.1",
  "description": "A terminal emulator for the 21st century.",
  "main": "src/main/main.js",
  "author": "Volodymyr Shatsky <shockone89@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/shockone/black-screen.git"
  },
  "bugs": {
    "url": "https://github.com/shockone/black-screen/issues"
  },
  "keywords": [
    "terminal",
    "emulator",
    "shell",
    "console"
  ],
  "dependencies": {
    "font-awesome": "4.3.0",
    "fuzzaldrin": "2.1.0",
    "jison": "0.4.15",
    "jquery": "2.1.4",
    "lodash": "3.9.3",
    "node-ansiparser": "1.0.2",
    "octicons": "2.2.0",
    "child_pty": "1.0.0",
    "react": "0.13.3",
    "react-json-inspector": "4.3.0",
    "rx": "2.5.2"
  },
  "devDependencies": {
    "babel": "5.4.7",
    "bower": "1.4.1",
    "chai": "2.3.0",
    "electron-packager": "4.1.2",
    "electron-prebuilt": "0.27.1",
    "electron-rebuild": "0.1.4",
    "gulp": "3.8.11",
    "gulp-babel": "5.1.0",
    "gulp-concat": "2.5.2",
    "gulp-react": "3.0.1",
    "gulp-sass": "2.0.1",
    "gulp-typescript": "2.7.5",
    "mocha": "2.2.5",
    "react-tools": "0.13.3",
    "typescript": "1.5.0-beta",
    "webdriverio": "git://github.com/webdriverio/webdriverio.git"
  },
  "scripts": {
    "install-all": "PATH=node_modules/.bin:$PATH rm -rf node_modules && npm update && npm install && electron-rebuild && bower install && npm run cleanup",
    "cleanup": "rm -rf build",
    "forest-run": "PATH=node_modules/.bin:$PATH gulp && electron .",
    "watch": "PATH=node_modules/.bin:$PATH gulp watch",
    "package": "PATH=node_modules/.bin:$PATH rm -rf '/Applications/Black Screen.app'; electron-packager . 'Black Screen' --platform=darwin --arch=x64 --version='0.27.1' --out='/Applications' --icon='./icon.icns'",
    "test": "PATH=node_modules/.bin:$PATH mocha --compilers js:babel/register"
  },
  "license": "MIT"
}
